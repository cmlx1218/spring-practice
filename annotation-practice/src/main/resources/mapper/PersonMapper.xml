<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tt.spring.annotation.test.persist.mapper.PersonMapper">

    <select id="getPersonById" resultType="com.tt.spring.annotation.test.pojo.dto.PersonDto">
        select *
        from userinfo
        where id = #{id}
    </select>

</mapper>