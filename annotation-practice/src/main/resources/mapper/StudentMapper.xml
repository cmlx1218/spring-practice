<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tt.spring.annotation.test.persist.mapper.StudentMapper">

    <select id="getStudentById" resultType="com.tt.spring.annotation.test.pojo.dto.StudentDto">
        select *
        from transaction_student
        where id = #{id}
    </select>

</mapper>